{
	"name": "adaptive-extender",
	"version": "0.2.14",
	"description": "Adaptive library for JS/TS development environments",
	"type": "module",
	"main": "./core/index.js",
	"types": "./core/index.d.ts",
	"exports": {
		".": {
			"import": "./core/index.js",
			"require": "./core/index.js",
			"types": "./core/index.d.ts"
		},
		"./core.js": {
			"import": "./core/index.js",
			"require": "./core/index.js",
			"types": "./core/index.d.ts"
		},
		"./core": "./core.js",
		"./node.js": {
			"import": "./node/index.js",
			"require": "./node/index.js",
			"types": "./node/index.d.ts"
		},
		"./node": "./node.js",
		"./web.js": {
			"import": "./web/index.js",
			"require": "./web/index.js",
			"types": "./web/index.d.ts"
		},
		"./web": "./web.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/eccs0103/adaptive-extender.git"
	},
	"keywords": [ "javascript", "typescript", "extensions", "core", "dom", "node" ],
	"author": "eccs0103",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/eccs0103/adaptive-extender/issues"
	},
	"homepage": "https://github.com/eccs0103/adaptive-extender#readme",
	"scripts": {
		"build:core": "tsc -p ./src/core/tsconfig.json",
		"build:node": "tsc -p ./src/node/tsconfig.json",
		"build:web": "tsc -p ./src/web/tsconfig.json",
		"build": "npm run build:core & npm run build:node & npm run build:web",
		"build:linker": "tsc -p ./src/tsconfig.json"
	},
	"devDependencies": {
		"typescript": "^5.9.2"
	}
}
