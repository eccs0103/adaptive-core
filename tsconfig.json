{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ES2020",
		"declaration": true,
		"sourceMap": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"allowJs": true,
		"forceConsistentCasingInFileNames": true,
	},
	"include": [ "**/*.ts" ],
	"exclude": [ "*.test.ts" ]
}
